      $('.color-preview-circle.clickable').click(function() {
        event.preventDefault(); 
        $(this).addClass('selected-prev').siblings().removeClass('selected-prev')
        var imageDisplay = $(this).parent().siblings().children('.slvr-col-img-wrapper').children('img')
        var newImage = $(this).data("image-src")
        var newId =  $(this).data("id")
        var newLifestyleDisplay = $("#" + newId + "lifestyle-wrapper");


        // change product url based on selected variant
        let urlWithoutId = $(this).parent().siblings('a').attr('href')
        if (urlWithoutId.includes('variant')) {
          // remove old variant id and replace with selected variant id from url
          let oldId = $(this).parent().siblings('a').attr('href').slice(-14)
          urlWithoutId = urlWithoutId.replace(`?variant=${oldId}`, '')
          let selectedVariantUrl = `${urlWithoutId}?variant=${newId}`
          $(this).parent().siblings('a').attr('href', selectedVariantUrl)
        } else {
          let selectedVariantUrl = `${urlWithoutId}?variant=${newId}`
          $(this).parent().siblings('a').attr('href', selectedVariantUrl)
        }

        $(imageDisplay).attr("src", newImage); 
        $(this).parent().siblings().children('.col-preview-bubble-wrapper:visible').hide();
        $(newLifestyleDisplay).show(); 
        $(this).parent().siblings().children('.col-preview-bubble-wrapper:visible').find('.col-preview-bubble.active').removeClass('active')
        $(this).parent().siblings().children('.col-preview-bubble-wrapper:visible').find('.col-preview-bubble:first-of-type').addClass('active')
      }); 
         

