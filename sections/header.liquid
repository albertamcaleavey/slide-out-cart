<script>
  // show product title and image when hovering over product name
  $('.mw-style').mouseover(function(){
    $('.product-preview').hide()
    let productImgClass = $(this).text().toLowerCase().replaceAll(' ', '-')
    if($(this).text() == 'Wraps') {
      $('.wraps').show()
    } else if($(this).text() == 'Shawls') {
      $('.shawls').show()
    } else if($(this).text() == 'Hats') {
      $('.hats').show()
    } else if ($(this).text() == 'Throw Blankets') {
      $('.throw-blankets').show()
    }  else if($('#women-drop-down').is(':visible')) {
      productImgClass = '.w-' + productImgClass
    } else if ($('#men-drop-down').is(':visible')) {
      productImgClass = '.m-' + productImgClass
    } 
    $(productImgClass).show()
    $('.product-title-nav').text($(this).text())
  })

  $('.mw-style').mouseleave(function() {
    $(this).addClass('nl')
    $('.product-preview').hide()
    if($('#women-drop-down').is(':visible')) {
      $('.w-crew').show()
    } else if ($('#men-drop-down').is(':visible')) {
     $('.m-crew').show()
    } 
    $('.product-title-nav').text('Crew')
  })
  
  $('.drop-link').on('click mouseover', function() {
    $('#women-drop-down, #men-drop-down, #kids-drop-down, #gifts-drop-down').hide();
    let currentLink = $(this).text()
     $('.product-title').text('Crew')
    if(currentLink == "Women") {
      $('#women-drop-down').show();
      $('.w-crew').show();
    } else if(currentLink == "Men") {
      $('#men-drop-down').show();
      $('.m-crew').show();
    } else if(currentLink == "Kids") {
      $('#kids-drop-down').show(); 
    } else {
      $('#gifts-drop-down').show(); 
    }
  });
  
  $('.reviews-link').mouseover(function() {
